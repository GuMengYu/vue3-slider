{
  "name": "vue3-slider",
  "version": "1.8.0",
  "author": "Freddie Nelson <freddie0208@hotmail.com> (https://freddienelson.co.uk)",
  "description": "A modern, customizable slider built entirely in Vue.js 3 with TypeScript in under 5KB (gzipped) and dependency free. (horizontal, vertical and circular slider)",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "slider",
    "range",
    "component",
    "ui",
    "keyboard support",
    "customizable",
    "typescript",
    "circular",
    "circle",
    "vertical",
    "horizontal",
    "0 dependencies"
  ],
  "homepage": "https://github.com/freddie-nelson/vue3-slider",
  "repository": {
    "type": "git",
    "url": "https://github.com/freddie-nelson/vue3-slider"
  },
  "bugs": {
    "url": "https://github.com/freddie-nelson/vue3-slider/issues",
    "email": "freddie0208@hotmail.com"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/vue3-slider.common.js",
  "types": "vue3-slider.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "serve:prod": "vue-cli-service serve dev-production-build/serve.ts",
    "build": "vue-cli-service build --mode production --target lib --name vue3-slider src/entry.ts",
    "build:demo": "vue-cli-service build dev/serve.ts --dest demo && node fixDemoHtml.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/resize-observer-browser": "^0.1.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",

    "@vue/cli-service": "~5.0.0",
    "vue-class-component": "^8.0.0-0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "typescript": "^4.3.5",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
